   "source": [
    "import requests\n",
    "import pandas as pd\n",
    "url = \"https://api.coindesk.com/v1/bpi/currentprice.json\"\n",
    "response = requests.get(url)\n",
    "\n",
    "# response\n",
    "\n",
    "if response.status_code == 200:\n",
    "    data = response.json()\n",
    "    print(data)\n",
    "\n",
    "else:\n",
    "    print(f\"Error: {response.status_code}\")\n",
    "\n",
    "pd.DataFrame(data)"
   ]
